{
  "name": "json-schema-faker",
  "version": "0.2.8",
  "description": "JSON-Schema + Faker",
  "homepage": "http://json-schema-faker.js.org",
  "main": "index.js",
  "scripts": {
    "dev": "jasmine-node spec --color --coffee --noStack --verbose --autotest --watch lib",
    "dev:lint": "eslint lib",
    "dev:spec": "jasmine-node spec --coffee --captureExceptions",
    "cover": "istanbul cover --root lib --x '**/spec/**' -- jasmine-node spec --coffee",
    "cover:up": "codecov --file coverage/lcov.info",
    "test": "npm run dev:lint && npm run dev:spec",
    "build": "node build-dist.js"
  },
  "contributors": [
    {
      "name": "Alvaro Cabrera",
      "email": "pateketrueke@gmail.com"
    },
    {
      "name": "Tomasz Ducin",
      "email": "tomasz@ducin.it",
      "url": "http://ducin.it"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/json-schema-faker/json-schema-faker"
  },
  "bugs": "https://github.com/json-schema-faker/json-schema-faker/issues",
  "license": "MIT",
  "dependencies": {
    "deref": "^0.6.3",
    "randexp": "^0.4.2"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "chance": "^0.8.0",
    "clone": "^1.0.2",
    "codecov": "^1.0.1",
    "eslint": "^1.10.3",
    "faker": "^3.0.1",
    "fs-extra": "^0.26.5",
    "glob": "^6.0.4",
    "istanbul": "^0.4.2",
    "jasmine-node": "^1.14.5",
    "jayschema": "^0.3.1",
    "lodash.template": "^4.0.1",
    "semver": "^5.1.0",
    "tv4": "^1.2.7",
    "uglify-js": "^2.6.1",
    "z-schema": "^3.16.1"
